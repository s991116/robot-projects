#
# Generated Makefile - do not edit!
#
# Edit the Makefile in the project folder instead (../Makefile). Each target
# has a -pre and a -post target defined where you can add customized code.
#
# This makefile implements configuration specific macros and targets.


# Environment
MKDIR=mkdir
CP=cp
GREP=grep
NM=nm
CCADMIN=CCadmin
RANLIB=ranlib
CC=gcc
CCC=g++
CXX=g++
FC=gfortran
AS=as

# Macros
CND_PLATFORM=GNU-Linux-x86
CND_DLIB_EXT=so
CND_CONF=Debug
CND_DISTDIR=dist
CND_BUILDDIR=build

# Include project Makefile
include Makefile

# Object Directory
OBJECTDIR=${CND_BUILDDIR}/${CND_CONF}/${CND_PLATFORM}

# Object Files
OBJECTFILES= \
	${OBJECTDIR}/Arduino.o \
	${OBJECTDIR}/Controller.o \
	${OBJECTDIR}/View.o \
	${OBJECTDIR}/main.o


# C Compiler Flags
CFLAGS=

# CC Compiler Flags
CCFLAGS=
CXXFLAGS=

# Fortran Compiler Flags
FFLAGS=

# Assembler Flags
ASFLAGS=

# Link Libraries and Options
LDLIBSOPTIONS=-Lncurses ../ArduinoController/dist/Debug/GNU-Linux-x86/libarduinocontroller.a -Wl,-rpath,../ArduinoCommand/dist/Debug/GNU-Linux-x86 -L../ArduinoCommand/dist/Debug/GNU-Linux-x86 -lArduinoCommand -Wl,-rpath,../CameraDetector/dist/Debug/GNU-Linux-x86 -L../CameraDetector/dist/Debug/GNU-Linux-x86 -lCameraDetector

# Build Targets
.build-conf: ${BUILD_SUBPROJECTS}
	"${MAKE}"  -f nbproject/Makefile-${CND_CONF}.mk ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/remotecontrolrobot

${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/remotecontrolrobot: ../ArduinoController/dist/Debug/GNU-Linux-x86/libarduinocontroller.a

${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/remotecontrolrobot: ../ArduinoCommand/dist/Debug/GNU-Linux-x86/libArduinoCommand.so

${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/remotecontrolrobot: ../CameraDetector/dist/Debug/GNU-Linux-x86/libCameraDetector.so

${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/remotecontrolrobot: ${OBJECTFILES}
	${MKDIR} -p ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}
	${LINK.cc} -o ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/remotecontrolrobot ${OBJECTFILES} ${LDLIBSOPTIONS} -lncurses -L$(HOME)/git/raspberrypi/userland/build/lib -lmmal_core -lmmal -l mmal_util -lvcos -lbcm_host -I/usr/include/opencv -lopencv_highgui -lopencv_core -lopencv_legacy -lopencv_video -lopencv_features2d -lopencv_calib3d -lopencv_imgproc

${OBJECTDIR}/Arduino.o: Arduino.cpp 
	${MKDIR} -p ${OBJECTDIR}
	${RM} $@.d
	$(COMPILE.cc) -g -I../ArduinoController -I../ArduinoCommand -I../CameraDetector -MMD -MP -MF $@.d -o ${OBJECTDIR}/Arduino.o Arduino.cpp

${OBJECTDIR}/Controller.o: Controller.cpp 
	${MKDIR} -p ${OBJECTDIR}
	${RM} $@.d
	$(COMPILE.cc) -g -I../ArduinoController -I../ArduinoCommand -I../CameraDetector -MMD -MP -MF $@.d -o ${OBJECTDIR}/Controller.o Controller.cpp

${OBJECTDIR}/View.o: View.cpp 
	${MKDIR} -p ${OBJECTDIR}
	${RM} $@.d
	$(COMPILE.cc) -g -I../ArduinoController -I../ArduinoCommand -I../CameraDetector -MMD -MP -MF $@.d -o ${OBJECTDIR}/View.o View.cpp

${OBJECTDIR}/main.o: main.cpp 
	${MKDIR} -p ${OBJECTDIR}
	${RM} $@.d
	$(COMPILE.cc) -g -I../ArduinoController -I../ArduinoCommand -I../CameraDetector -MMD -MP -MF $@.d -o ${OBJECTDIR}/main.o main.cpp

# Subprojects
.build-subprojects:
	cd ../ArduinoController && ${MAKE}  -f Makefile CONF=Debug
	cd ../ArduinoCommand && ${MAKE}  -f Makefile CONF=Debug
	cd ../CameraDetector && ${MAKE}  -f Makefile CONF=Debug

# Clean Targets
.clean-conf: ${CLEAN_SUBPROJECTS}
	${RM} -r ${CND_BUILDDIR}/${CND_CONF}
	${RM} ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/remotecontrolrobot

# Subprojects
.clean-subprojects:
	cd ../ArduinoController && ${MAKE}  -f Makefile CONF=Debug clean
	cd ../ArduinoCommand && ${MAKE}  -f Makefile CONF=Debug clean
	cd ../CameraDetector && ${MAKE}  -f Makefile CONF=Debug clean

# Enable dependency checking
.dep.inc: .depcheck-impl

include .dep.inc
